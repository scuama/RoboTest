    chocolate_pudding_1 - chocolate_pudding
    cream_cheese_1 cream_cheese_2 - cream_cheese
    basket_1 - basket
  alphabet_soup_obstacle_1 - alphabet_soup
  ketchup_obstacle_1 - ketchup
  red_coffee_mug_obstacle_1 - red_coffee_mug
  )

  (:obj_of_interest
    cream_cheese_1
    cream_cheese_2
    basket_1
  )

  (:init
    (On chocolate_pudding_1 living_room_table_chocolate_pudding_init_region)
    (On cream_cheese_1 chocolate_pudding_1)
    (On cream_cheese_2 cream_cheese_1)
    (On basket_1 living_room_table_basket_init_region)
  (On alphabet_soup_obstacle_1 living_room_table_alphabet_soup_obstacle_1_init_region)
  (On ketchup_obstacle_1 living_room_table_ketchup_obstacle_1_init_region)
  (On red_coffee_mug_obstacle_1 living_room_table_red_coffee_mug_obstacle_1_init_region)
  )

  (:goal
    (And (In cream_cheese_1 basket_1_contain_region)
         (In cream_cheese_2 basket_1_contain_region))
  )

)
